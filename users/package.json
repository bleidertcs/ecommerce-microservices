{
    "name": "@ecommerce/users",
    "version": "1.1.0",
    "packageManager": "pnpm@10.2.0",
    "description": "",
    "private": true,
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "build:prod": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "node dist/main",
        "dev": "npm run proto:generate && nest start --watch",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "test": "jest --config test/jest.json --runInBand",
        "proto:generate": "npx nestjs-grpc generate --proto ./src/protos --output ./src/generated",
        "prisma:generate": "dotenv -e .env.docker -- prisma generate",
        "prisma:migrate": "dotenv -e .env.docker -- prisma migrate dev",
        "prisma:migrate:prod": "dotenv -e .env.docker -- prisma migrate deploy",
        "prisma:studio": "dotenv -e .env.docker -- prisma studio",
        "prisma:seed": "dotenv -e .env.docker -- ts-node prisma/seed.ts",
        "prepare": "husky"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.13.4",
        "@grpc/proto-loader": "^0.7.15",
        "@keyv/redis": "^4.4.0",
        "@nestjs/cache-manager": "^3.0.1",
        "@nestjs/common": "^10.4.6",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.4.6",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/microservices": "^10.4.6",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.4.6",
        "@nestjs/swagger": "^8.0.1",
        "@nestjs/terminus": "^10.2.3",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/auto-instrumentations-node": "^0.55.0",
        "@opentelemetry/exporter-metrics-otlp-grpc": "^0.57.2",
        "@opentelemetry/exporter-trace-otlp-grpc": "^0.57.0",
        "@opentelemetry/resources": "^1.30.0",
        "@opentelemetry/sdk-metrics": "^1.30.1",
        "@opentelemetry/sdk-node": "^0.57.0",
        "@opentelemetry/sdk-trace-node": "^1.30.0",
        "@opentelemetry/semantic-conventions": "^1.28.0",
        "@prisma/client": "^5.21.1",
        "@pyroscope/nodejs": "^0.4.0",
        "@sentry/node": "^9.26.0",
        "amqp-connection-manager": "^4.1.14",
        "amqplib": "^0.10.4",
        "bcryptjs": "^2.4.3",
        "cache-manager": "^6.4.3",
        "cacheable": "^1.9.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "express": "^4.21.1",
        "helmet": "^8.0.0",
        "ioredis": "^5.6.1",
        "joi": "^17.13.3",
        "nest-winston": "^1.10.2",
        "nestjs-grpc": "1.3.11",
        "nestjs-i18n": "^10.4.9",
        "opossum": "^8.4.1",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "prisma": "^5.21.1",
        "prom-client": "^15.1.3",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.0.0",
        "@eslint/js": "^9.0.0",
        "@faker-js/faker": "^9.1.0",
        "@nestjs/cli": "^10.4.5",
        "@nestjs/schematics": "^10.2.3",
        "@nestjs/testing": "^10.4.6",
        "@types/bcryptjs": "^2.4.6",
        "@types/cache-manager-ioredis": "^2.0.7",
        "@types/config": "^3.3.5",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/joi": "^17.2.3",
        "@types/mime": "^3.0.4",
        "@types/node": "^22.8.5",
        "@types/passport-jwt": "^4.0.1",
        "@types/pg": "^8.11.10",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.9.1",
        "@typescript-eslint/parser": "^7.9.1",
        "dotenv-cli": "^8.0.0",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-no-relative-import-paths": "^1.5.5",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^9.1.6",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.33.1"
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write",
            "eslint --fix"
        ]
    }
}
