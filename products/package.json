{
    "name": "@ecommerce/products",
    "version": "1.1.0",
    "packageManager": "pnpm@10.28.0",
    "description": "",
    "private": true,
    "engines": {
        "node": "^22.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build && tsc prisma/seed.ts --outDir dist/prisma --module commonjs --esModuleInterop --skipLibCheck",
        "build:prod": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "node dist/main",
        "dev": "npm run proto:generate && nest start --watch",
        "lint": "eslint \"{src,test}/**/*.ts\" --fix",
        "test": "jest --config test/jest.json --runInBand",
        "proto:generate": "npx nestjs-grpc generate --proto ./src/protos --output ./src/generated",
        "prisma:generate": "dotenv -e .env -- prisma generate",
        "prisma:migrate": "dotenv -e .env -- prisma migrate dev",
        "prisma:migrate:prod": "dotenv -e .env -- prisma migrate deploy",
        "prisma:studio": "dotenv -e .env -- prisma studio",
        "prisma:seed": "node dist/prisma/seed.js",
        "prepare": "husky"
    },
    "dependencies": {
        "@faker-js/faker": "^10.3.0",
        "@grpc/grpc-js": "^1.14.3",
        "@grpc/proto-loader": "^0.8.0",
        "@keyv/redis": "^5.1.6",
        "@nestjs/cache-manager": "^3.1.0",
        "@nestjs/common": "^11.1.14",
        "@nestjs/config": "^4.0.3",
        "@nestjs/core": "^11.1.14",
        "@nestjs/jwt": "^11.0.2",
        "@nestjs/microservices": "^11.1.14",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.1.14",
        "@nestjs/swagger": "^11.2.6",
        "@nestjs/terminus": "^11.1.1",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/api-logs": "^0.212.0",
        "@opentelemetry/auto-instrumentations-node": "^0.70.0",
        "@opentelemetry/exporter-logs-otlp-grpc": "^0.212.0",
        "@opentelemetry/exporter-metrics-otlp-grpc": "^0.212.0",
        "@opentelemetry/exporter-trace-otlp-grpc": "^0.212.0",
        "@opentelemetry/instrumentation-winston": "^0.56.0",
        "@opentelemetry/resources": "^2.5.1",
        "@opentelemetry/sdk-logs": "^0.212.0",
        "@opentelemetry/sdk-metrics": "^2.5.1",
        "@opentelemetry/sdk-node": "^0.212.0",
        "@opentelemetry/sdk-trace-node": "^2.5.1",
        "@opentelemetry/semantic-conventions": "^1.39.0",
        "@opentelemetry/winston-transport": "^0.22.0",
        "@prisma/client": "^7.4.1",
        "@prisma/instrumentation": "^7.4.1",
        "@sentry/node": "^10.39.0",
        "@types/node": "^25.3.0",
        "amqp-connection-manager": "^5.0.0",
        "amqplib": "^0.10.9",
        "bcryptjs": "^3.0.3",
        "cache-manager": "^7.2.8",
        "cacheable": "^2.3.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.3",
        "express": "^5.2.1",
        "helmet": "^8.1.0",
        "ioredis": "^5.9.3",
        "joi": "^18.0.2",
        "nats": "^2.29.3",
        "nest-winston": "^1.10.2",
        "nestjs-grpc": "1.5.2",
        "nestjs-i18n": "^10.6.0",
        "opossum": "^9.0.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "prisma": "^7.4.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "winston": "^3.19.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.0.0",
        "@eslint/js": "^9.0.0",
        "@nestjs/cli": "^10.4.5",
        "@nestjs/schematics": "^10.2.3",
        "@nestjs/testing": "^10.4.6",
        "@types/bcryptjs": "^2.4.6",
        "@types/cache-manager-ioredis": "^2.0.7",
        "@types/config": "^3.3.5",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/joi": "^17.2.3",
        "@types/mime": "^3.0.4",
        "@types/node": "^22.8.5",
        "@types/passport-jwt": "^4.0.1",
        "@types/pg": "^8.11.10",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.9.1",
        "@typescript-eslint/parser": "^7.9.1",
        "dotenv-cli": "^8.0.0",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-no-relative-import-paths": "^1.5.5",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^9.1.6",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.33.1"
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "prisma": {
        "seed": "node dist/prisma/seed.js"
    }
}
